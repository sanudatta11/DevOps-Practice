aws iam create-role --role-name vmimport --assume-role-policy-document file://trustpolicy.json
aws iam put-role-policy --role-name vmimport --policy-name vmimport --policy-document file://trust-policy.json
aws ec2 import-image --description "OVA Migration" --disk-containers file://containers.json
aws ec2 describe-image-import-tasks --import-task-ids <ID>